{   
    "env": {
        "browser": true,
        "es2021": true
    }, 
    "ecmaFeatures": {
    "blockBindings": true,
    "jsx": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    
    "rules": {
        "react/jsx-props-no-spreading":0,
        "global-require": 0 ,
        "no-loop-func": 0,
        "react/prop-types": 0,
        "react/react-in-jsx-scope": 0,
        "react/jsx-uses-react": 0,
        "no-await-in-loop":0,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "indent": [
            "error",
            2
        ],
        "linebreak-style": 0,
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-promise-executor-return":0,          
        "react/no-array-index-key":0,  
        "no-unused-vars":0,
        "array-callback-return":0
    }
}